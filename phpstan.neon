parameters:
    level: 5
    paths:
        - app
    excludePaths:
        - vendor
    bootstrapFiles:
        - bootstrap/app.php
    ignoreErrors:
        - '#Undefined method.*::(orders|reviews|loginHistories|totalSpent|getActiveSessionCount|getSessionStats|getSecuritySettings|updateSecuritySettings|recentLoginAttempts|isAdmin)\(\)#'
        - '#Call to an undefined method Illuminate\\Contracts\\Auth\\Authenticatable::(update|save|isDirty)\(\)#'
        - '#Undefined property: Illuminate\\Contracts\\Auth\\Authenticatable::\$id#'
    checkMissingIterableValueType: false